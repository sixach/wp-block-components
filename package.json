{
	"name": "@sixa/wp-block-components",
	"version": "1.14.1",
	"description": "A collection of most used React components crafted for the sixa projects.",
	"keywords": [
		"sixa",
		"gutenberg",
		"wordpress",
		"components",
		"react"
	],
	"homepage": "https://github.com/sixach/wp-block-components#readme",
	"bugs": {
		"url": "https://github.com/sixach/wp-block-components/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sixach/wp-block-components.git"
	},
	"license": "GPL-3.0-or-later",
	"author": "sixa AG",
	"sideEffects": false,
	"main": "build/index.js",
	"module": "build/index.js",
	"scripts": {
		"build": "cross-env BABEL_ENV=default NODE_ENV=production && rm -rf build && mkdir build && babel src --out-dir build --no-copy-ignored",
		"check-engines": "wp-scripts check-engines",
		"check-licenses": "wp-scripts check-licenses",
		"format": "wp-scripts format",
		"lint:js": "wp-scripts lint-js",
		"lint:js-fix": "wp-scripts lint-js --fix",
		"lint:md:docs": "wp-scripts lint-md-docs",
		"lint:md:js": "wp-scripts lint-md-js",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"packages-update": "wp-scripts packages-update",
		"prepublishOnly": "npm run format && npm run build",
		"test:e2e": "wp-scripts test-e2e",
		"test:unit": "wp-scripts test-unit-js",
		"storybook": "start-storybook -p 6006",
		"deploy-storybook": "storybook-to-ghpages",
		"build-storybook": "build-storybook",
		"prepare": "husky install"
	},
	"lint-staged": {
		"*.{js,ts,tsx}": [
			"npm run lint:js",
			"npm run format"
		]
	},
	"dependencies": {
		"@emotion/styled": "11.8.1",
		"@sixa/icon-library": "1.3.1",
		"@sixa/wp-block-utils": "1.2.0",
		"@sixa/wp-react-hooks": "1.14.0",
		"@wordpress/api-fetch": "5.2.7",
		"@wordpress/blob": "3.8.0",
		"@wordpress/block-editor": "8.6.0",
		"@wordpress/components": "19.10.0",
		"@wordpress/compose": "5.6.0",
		"@wordpress/data": "6.8.0",
		"@wordpress/element": "4.6.0",
		"@wordpress/hooks": "3.8.0",
		"@wordpress/html-entities": "3.8.0",
		"@wordpress/i18n": "4.8.0",
		"@wordpress/icons": "6.3.0",
		"@wordpress/primitives": "3.6.0",
		"@wordpress/url": "3.9.0",
		"array-move": "4.0.0",
		"classnames": "2.3.1",
		"clean-deep": "3.4.0",
		"glider-js": "1.7.7",
		"html-react-parser": "1.4.14",
		"is-dark-color": "1.2.0",
		"lodash": "4.17.21",
		"prop-types": "15.8.1",
		"random-hex-color-generator": "1.0.12",
		"react-easy-sort": "1.5.0",
		"react-glider": "2.2.1"
	},
	"devDependencies": {
		"@babel/cli": "7.17.10",
		"@babel/core": "7.18.2",
		"@babel/preset-env": "7.18.2",
		"@babel/preset-react": "7.17.12",
		"@storybook/addon-a11y": "6.5.7",
		"@storybook/addon-essentials": "6.5.7",
		"@storybook/addon-links": "6.5.7",
		"@storybook/react": "6.5.7",
		"@storybook/storybook-deployer": "2.8.11",
		"@storybook/theming": "6.5.7",
		"@wordpress/scripts": "22.5.0",
		"babel-loader": "8.2.5",
		"cross-env": "7.0.3",
		"husky": "7.0.4",
		"lint-staged": "12.5.0",
		"prettier": "npm:wp-prettier@2.2.1-beta-1"
	},
	"publishConfig": {
		"access": "public"
	}
}
