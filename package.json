{
	"name": "@sixach/wp-block-components",
	"version": "1.5.2-beta.0",
	"description": "A collection of most used React components crafted for the sixa projects.",
	"keywords": [
		"gutenberg",
		"wordpress",
		"components",
		"react"
	],
	"homepage": "https://github.com/sixach/wp-block-components#readme",
	"bugs": {
		"url": "https://github.com/sixach/wp-block-components/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sixach/wp-block-components.git"
	},
	"license": "GPL-3.0-or-later",
	"author": "sixa AG",
	"sideEffects": false,
	"main": "build/index.js",
	"module": "build/index.js",
	"scripts": {
		"build": "cross-env BABEL_ENV=default NODE_ENV=production && rm -rf build && mkdir build && babel src --out-dir build --no-copy-ignored",
		"check-engines": "wp-scripts check-engines",
		"check-licenses": "wp-scripts check-licenses",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:css-fix": "wp-scripts lint-style --fix",
		"lint:js": "wp-scripts lint-js",
		"lint:js-fix": "wp-scripts lint-js --fix",
		"lint:md:docs": "wp-scripts lint-md-docs",
		"lint:md:js": "wp-scripts lint-md-js",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"packages-update": "wp-scripts packages-update",
		"prepublishOnly": "npm run format && npm run build",
		"test:e2e": "wp-scripts test-e2e",
		"test:unit": "wp-scripts test-unit-js",
		"storybook": "start-storybook -p 6006",
		"deploy-storybook": "storybook-to-ghpages",
		"build-storybook": "build-storybook",
		"prepare": "husky install"
	},
	"lint-staged": {
		"*.{scss,css}": [
			"npm run lint:css"
		],
		"*.{js,ts,tsx}": [
			"npm run lint:js",
			"npm run format"
		]
	},
	"dependencies": {
		"@emotion/styled": "^11.3.0",
		"@sixa/wp-react-hooks": "1.4.1-beta.1",
		"@sixach/icon-library": "1.1.1-beta.0",
		"@wordpress/api-fetch": "^5.2.2",
		"@wordpress/blob": "^3.2.1",
		"@wordpress/block-editor": "^7.0.2",
		"@wordpress/components": "^17.0.0",
		"@wordpress/compose": "^5.0.2",
		"@wordpress/element": "^4.0.1",
		"@wordpress/html-entities": "^3.2.1",
		"@wordpress/i18n": "^4.2.2",
		"@wordpress/icons": "^5.0.2",
		"@wordpress/primitives": "^3.0.1",
		"@wordpress/url": "^3.2.2",
		"array-move": "^3.0.1",
		"glider-js": "^1.7.7",
		"html-react-parser": "^1.3.0",
		"prop-types": "^15.7.2",
		"react-easy-sort": "^1.3.1",
		"react-glider": "^2.2.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.15.4",
		"@babel/core": "^7.15.5",
		"@babel/preset-env": "^7.15.6",
		"@babel/preset-react": "7.14.5",
		"@sixa/wp-block-utils": "1.0.4-beta.0",
		"@storybook/addon-a11y": "^6.3.8",
		"@storybook/addon-essentials": "^6.3.8",
		"@storybook/addon-links": "^6.3.8",
		"@storybook/react": "^6.3.8",
		"@storybook/storybook-deployer": "2.8.10",
		"@storybook/theming": "^6.3.8",
		"@wordpress/scripts": "^18.0.1",
		"babel-loader": "8.2.2",
		"classnames": "2.3.1",
		"cross-env": "7.0.3",
		"husky": "^7.0.2",
		"is-dark-color": "1.2.0",
		"lint-staged": "11.1.2",
		"lodash": "4.17.21",
		"random-hex-color-generator": "1.0.12",
		"wp-prettier": "^2.2.1-beta-1"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	}
}
